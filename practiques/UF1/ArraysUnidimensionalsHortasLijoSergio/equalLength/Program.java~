public class Program {
    
    /**
     * 
     * @param args not used
     * 
     */
    public boolean equalLength (double[]a, double[]b) {
        boolean equal = false
            System.out.print("\n\n EQUAL LENGTH  \n\n");        
        // Iniciamos el bucle con la longitud del array
        if (a.length == b.length) {     
            equal = true;           
        }   
        return equal;
    }
}

