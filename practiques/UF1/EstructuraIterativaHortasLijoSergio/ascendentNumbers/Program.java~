public class Program { 
 
    /** 
     * Escribe una secuencia entera
     *  
     * @param n1 an int 
     * @param n2 an int 
     */ 
    public void writeAscNumbers(int n1, int n2) { 
        int x; 
        i = n1; 
        while (i <= n2) { 
            System.out.print(i + " "); 
            i++; 
        } 
        System.out.println(); 
    } 
 
}