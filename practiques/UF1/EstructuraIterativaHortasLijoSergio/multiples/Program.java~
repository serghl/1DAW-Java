public class Program { 
    
    /** 
     * @n1 an int to be add
     * @n2 an int to be add
     *  
     */ 
    public void writeMultiples(int n1, int n2) {
        int resultado = n1;
        for (int i = 1; (resultado + n1) <= n2; i++) {
            resultado = n1 * i;
            System.out.println(resultado);
        }
    } 
}