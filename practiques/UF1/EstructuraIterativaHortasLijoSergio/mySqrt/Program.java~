public class Program { 
    
    /** 
     * @n1 an int to be add
     * @return the sqrt
     */ 
    public int mySqrt(int n1) {
        int i = 1;
        int temp = 0;
        // Iniciamos un bucle, multiplicado hasta no superar n1
        while (temp <= n1) {
            temp = i*i;
            if (temp <= n1) {
                i++;
            }
        }
        return i;
    }
}




