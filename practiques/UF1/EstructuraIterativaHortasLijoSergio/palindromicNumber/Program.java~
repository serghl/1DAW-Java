public class Program { 
    
    /** 
     * @n1 an int to be add
     *  
     */ 
    public boolean isPalindromic(int n1) {
        int temp = 0;
        int x;
        int y;
        while (int i = 1; i <= n1 ; i++) {
            if (n1 % i == 0) {
                System.out.println(i);
            }
        }
    }
}

