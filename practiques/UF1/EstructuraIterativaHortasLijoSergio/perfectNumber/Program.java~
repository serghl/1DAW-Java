public class Program { 
    
    /** 
     * @n1 an int to be add
     *  
     */ 
    public boolean isPerfectNumber (int n1) {
        boolean isPerfect = false;      
        for (int i = 1; i <= n1 ; i++) {
            if (n1 % i == 0) {
                System.out.println(i);
            }
        }
        }        
    }
}






