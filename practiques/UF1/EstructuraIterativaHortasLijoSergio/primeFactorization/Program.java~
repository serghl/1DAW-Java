public class Program { 
    
    /** 
     * @n1 an int to be add
     * @return the sqrt
     */ 
    public int primeFactorization(int n1) {
        // Iniciamos un bucle, multiplicado hasta no superar n1
        for (int i = 0; i <= n1; i++) {
            temp = i*i;
            if (temp <= n1) {
                i++;
            }
        }
        return i;
    }
}




