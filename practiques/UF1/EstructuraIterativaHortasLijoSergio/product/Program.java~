public class Program { 
    
    /** 
     * @n1 an int to be add
     * @n2 an int to be add
     *  
     */ 
    public void multiply(int n1, int n2) {
        // Variable de resultado
        int resultado = 0;
        for (int i = 0; i <= n2 ; i++) {
          resultado = resultado + n1;
          System.out.println(n1" + \n");  
            }
        }
        System.out.println(resultado);
    }
}

