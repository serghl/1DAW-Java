public class Program { 
    
    /** 
     * @n an int to be add
     * 
     */ 
    public inr sumDigits (int n) {
        int total;
        int t = n;
        int t2 ;
        while (t > 0) {
        t2 = t % 10;    
        total = total + t2;
        t / 10;                              
        }
        return sumDigits;
    }
}




