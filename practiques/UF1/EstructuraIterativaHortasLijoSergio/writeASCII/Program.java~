public class Program { 
    
    /** 
     *  Escribe una secuencia descendente del codigo ASCII
     *  
     * @param args not used 
     */ 
    public void writeASCII() { 
        int i = 127;
         while (i < 256) { 
            System.out.printf("%c\n", i); 
            i++; 
        }
    } 
}