public class Program {
    
    /**
     * 
     * @param dist an int
     * @param time an int
     * @return the speed in Km/h
     * 
     */
    public double velocity (int dist,int time) {
        int speed, int km, int hour;
        km = dist / 1000;
        hour = time / 3600;
        speed = time / speed;
        return speed;
    }
}